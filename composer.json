{
    "name": "keyxmare/ddd-maker-bundle",
    "description": "Symfony MakerBundle extension for DDD / Clean Architecture scaffolding",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": ">8.1",
        "symfony/maker-bundle": "^1.0",
        "symfony/console": "^6.0 || ^7.0"
    },
    "autoload": {
        "psr-4": {
            "Keyxmare\\DomainDrivenDesignMakerBundle\\": "src/"
        }
    },
    "extra": {
        "symfony": {
            "bundle-class": "Keyxmare\\DomainDrivenDesignMakerBundle\\KeyxmareDomainDrivenDesignMakerBundle"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.86"
    }
}
